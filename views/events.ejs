<% include partials/header %>

<!-- Coming Soon Page -->
<section class="probootstrap-cover overflow-hidden relative"  style="background-image: url('assets/images/eventbackground.jpg');" data-stellar-background-ratio=  id="section-home">
    <!-- <div class="overlay"></div> -->
    <div class="container">
      <div class="row align-items-center" style="text-align: center;">
        <div class="col-md">
          <h1 class="heading mb-2 display-4 font-light probootstrap-animate">Coming Soon</h1>
          <p class="lead mb-5 probootstrap-animate">25th - 27th Jan 2019</p>
          <p class="probootstrap-animate">
            <a href="index.html#section-contact" role="button" class="btn btn-primary p-3 mr-3 pl-5 pr-5 text-uppercase d-lg-inline d-md-inline d-sm-block d-block mb-3">For notifications</a> 
          </p>
        </div>         
      </div> 
    </div>
  </section>

  <section class="probootstrap_section" id="section-city-guides">
    <div class="container">
      <div class="row mb-3">
        
          
          <% events.forEach(function(events){ %>
            <div class="col-md-6">  
          <div class="media probootstrap-media d-flex align-items-stretch mb-4 probootstrap-animate">
             <img class="probootstrap-media-image" src="<%= events.img %>">
            <div class="media-body probootstrap-animate">
              <h5 class="mb-3 probootstrap-animate"><%= events.name %></h5>
              <h6 class="probootstrap-animate">Price Worth:</h6>
              <p class="probootstrap-animate">Winner: Rs.<%= events.FP %>/-</p>
              <p class="probootstrap-animate">1st Runnerup: Rs.<%= events.SP %>/-</p>
              <p class="probootstrap-animate">2st Runnerup: Rs.<%= events.TP %>/-</p>
              <p class="probootstrap-animate">
                  <a href="#section-feature-testimonial" role="button" class="btn btn-primary p-3 mr-3 pl-5 pr-5 text-uppercase d-lg-inline d-md-inline d-sm-block d-block mb-3">About the Wings</a> 
                </p>
            </div>
          </div>
        </div>
          
          <% }) %>
      </div>
    </div>
  </section>

  <!-- END section -->

  <% include partials/footer %>
